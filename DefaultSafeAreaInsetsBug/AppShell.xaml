<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="DefaultSafeAreaInsetsBug.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:DefaultSafeAreaInsetsBug"
    Shell.FlyoutBehavior="Disabled"
    Title="DefaultSafeAreaInsetsBug">

    <TabBar>
        <!--CheckboxBlankCircle-->

        <Tab Title="VL">
            <ShellContent Title="Home2"
                          ContentTemplate="{DataTemplate local:MainPage2}"
                          Route="MainPage2" />
        </Tab>
        
        <Tab Title="SV_VL">
            <ShellContent Title="Home"
                          ContentTemplate="{DataTemplate local:MainPage}"
                          Route="MainPage" />
        </Tab>


        <Tab Title="Grid_SV_VL">
            <ShellContent Title="Home3"
                          ContentTemplate="{DataTemplate local:MainPage3}"
                          Route="MainPage3" />
        </Tab>
        
    </TabBar>
</Shell>
